{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=(localdb)\\MSSQLLocalDB;Database=Umbraco9-Blazor;Integrated Security=true"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "f3fdb78f-6bfa-484d-92c7-64440eee9dce"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsDirectory": "~/Models/UmbracoModelsBuilder/",
        "ModelsNamespace": "Umbraco9_Blazor.Models.UmbracoModelsBuilder",
        "AcceptUnsafeModelsDirectory": true
      }
    }
  }
}